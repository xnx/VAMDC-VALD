							<math xmlns="http://www.w3.org/1998/Math/MathML">
								<apply>
									<leq/>
									<apply>
										<abs/>
										<apply>
											<minus/>
											<ci>ltcs:v1#InitialStateRef</ci>
											<ci>ltcs:v1#FinalStateRef</ci>
										</apply>
									</apply>
									<cn>10</cn>
