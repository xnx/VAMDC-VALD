											<cn>1</cn>
										</apply>
									</apply>
									<apply>
										<and/>
										<apply>
											<neq/>
											<ci>ltcs:kronigParity#InitialStateRef</ci>
											<ci>ltcs:kronigParity#FinalStateRef</ci>
										</apply>
										<apply>
											<eq/>
											<apply>
												<abs/>
												<apply>
													<minus/>
													<ci>ltcs:J#InitialStateRef</ci>
													<ci>ltcs:J#FinalStateRef</ci>
												</apply>
											</apply>
											<cn>0</cn>
										</apply>
									</apply>
								</apply>
							</math>
						</rule>
					</xs:appinfo>
				</xs:annotation>
